# Ambiente
NODE_ENV=development

# Servidor
PORT=3000
CORS_ORIGIN=http://localhost:3000

# Banco de dados
DATABASE_URL="mysql://user:password@localhost:3306/advancemais"

# Segurança
JWT_SECRET=your-jwt-secret-key
JWT_EXPIRES_IN=1d
BCRYPT_SALT_ROUNDS=10

# Credenciais para acessar a documentação da API
DOCS_USERNAME=admin
DOCS_PASSWORD=sua-senha-super-secreta

# Logging
LOG_LEVEL=info

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100